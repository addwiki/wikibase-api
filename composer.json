{
	"name": "addwiki/wikibase-api",
	"type": "library",
	"description": "A Wikibase api lib",
	"keywords": ["Mediawiki","wikibase","wikidata"],
	"license": "GPL-2.0+",
	"authors": [
		{
			"name": "Addshore"
		}
	],
	"require": {
		"php": ">=5.5",
		"addwiki/mediawiki-api-base": "~2.0",
		"addwiki/mediawiki-datamodel": "~0.5.0|~0.6.0",
		"serialization/serialization": "~3.2",
		"wikibase/data-model": "~4.2",
		"wikibase/data-model-services": "~3.0",
		"wikibase/data-model-serialization": "~2.0"
	},
	"require-dev": {
		"data-values/data-values": "~1.0.0",
		"data-values/common": "~0.3.0",
		"data-values/geo": "~1.0|~0.2.0",
		"data-values/number": "~0.5.0",
		"data-values/time": "~0.8.0",
		"data-values/validators": "~0.1.0",
		"data-values/serialization": "~1.0"
	},
	"autoload": {
		"psr-4": {
			"Wikibase\\": "src/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "0.7.x-dev"
		}
	}
}
